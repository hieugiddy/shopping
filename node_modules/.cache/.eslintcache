ntAnchorEl={null}\r\n      >\r\n        <MenuItem onClick={handleCloseMenu}>Profile</MenuItem>\r\n        <MenuItem onClick={handleCloseMenu}>My account</MenuItem>\r\n        <MenuItem onClick={handleLogoutAction}>Logout</MenuItem>\r\n      </Menu>\r\n      <Dialog\r\n        open={open}\r\n        onClose={handleClose}\r\n        aria-labelledby=\"form-dialog-title\"\r\n      >\r\n        {mode === MODE.LOGIN && (\r\n          <>\r\n            <Login handleClose={handleClose} />\r\n            <Button color=\"primary\" onClick={() => setMode(MODE.REGISTER)}>\r\n              Don't you have account ? Sign up\r\n            </Button>\r\n          </>\r\n        )}\r\n        {mode === MODE.REGISTER && (\r\n          <>\r\n            <Register handleClose={handleClose} />\r\n            <Button color=\"primary\" onClick={() => setMode(MODE.LOGIN)}>\r\n              Do you have account ? Sign in\r\n            </Button>\r\n          </>\r\n        )}\r\n      </Dialog>\r\n    </div>\r\n  );\r\n}\r\n","D:\\Source\\ReactJs\\react-app\\src\\constants\\storageKey.js",[],"D:\\Source\\ReactJs\\react-app\\src\\api\\userApi.js",[],"D:\\Source\\ReactJs\\react-app\\src\\feature\\Cart\\index.js",[],"D:\\Source\\ReactJs\\react-app\\src\\components\\NotFound\\index.js",[],"D:\\Source\\ReactJs\\react-app\\src\\components\\Todo\\index.js",[],"D:\\Source\\ReactJs\\react-app\\src\\feature\\Product\\index.js",[],"D:\\Source\\ReactJs\\react-app\\src\\feature\\Cart\\selectors.js",[],"D:\\Source\\ReactJs\\react-app\\src\\api\\axiosClient.js",[],"D:\\Source\\ReactJs\\react-app\\src\\feature\\Cart\\component\\CartItem.js",["240","241"],"import {\r\n  Box,\r\n  Button,\r\n  ButtonGroup,\r\n  Grid,\r\n  Link,\r\n  makeStyles,\r\n  Paper,\r\n  Typography,\r\n} from \"@material-ui/core\";\r\nimport React from \"react\";\r\nimport { useDispatch } from \"react-redux\";\r\nimport { STATIC_HOST, THUMBNAIL_PLACEHOLDER } from \"../../../constants\";\r\nimport { formatPrice } from \"../../../utils/co